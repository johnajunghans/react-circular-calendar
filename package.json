{
  "name": "react-circular-calendar",
  "private": false,
  "version": "0.1.0",
  "description": "Typed React components for a circular (wheel) calendar with events and selection.",
  "keywords": ["react", "calendar", "circular", "wheel", "svg", "component", "library"],
  "license": "MIT",
  "author": "johnajunghans",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnajunghans/react-circular-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/johnajunghans/react-circular-calendar/issues"
  },
  "homepage": "https://github.com/johnajunghans/react-circular-calendar#readme",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "dist/index.umd.cjs",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "dependencies": {
    "d3-shape": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-shape": "^3.1.7",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  }
}
